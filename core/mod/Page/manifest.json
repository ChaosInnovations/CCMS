{
    "module_data": {
        "name":"Page",
        "description":"Manages web page storage, editing, and display",
        "type":"Module",
        "version":[1, 0, 0, 0],
        "release_date":"2019-06-23",
        "author": {
            "name":"Thomas Boland",
            "email":"hello@thomasboland.me",
            "website":"http://ccms.thomasboland.me"
        }
    },

    "dependencies": {
        "libraries": [
            {"name":"CCMS", "min_version":[1, 9, 1, 0]},
            {"name":"ContentType", "min_version":[1, 0, 0, 0]}
        ],
        "modules": [
            {"name":"Bootstrap", "min_version":[4, 1, 1, 0]},
            {"name":"Database", "min_version":[1, 0, 0, 0]},
            {"name":"FontAwesome", "min_version":[5, 0, 13, 0]},
            {"name":"JQuery", "min_version":[3, 3, 1, 0]},
            {"name":"ModuleMenu", "min_version":[1, 0, 0, 0]},
            {"name":"SecureMenu", "min_version":[1, 0, 0, 0]},
            {"name":"SiteConfiguration", "min_version":[1, 0, 0, 0]},
            {"name":"User", "min_version":[1, 0, 0, 0]}
        ]
    },

    "routes": [
        {"rank":200, "hook":"/^web:\\/?api\\/page\\/new\\/?$/i",      "target":"\\Mod\\Page::hookNewPage",    "description":"Creates a new page"},
        {"rank":200, "hook":"/^web:\\/?api\\/page\\/remove\\/?$/i",   "target":"\\Mod\\Page::hookRemovePage", "description":"Removes a page"},
        {"rank":200, "hook":"/^web:\\/?api\\/page\\/edit\\/?$/i",     "target":"\\Mod\\Page::hookEditPage",   "description":"Edits a page"},
        {"rank":200, "hook":"/^web:\\/?api\\/page\\/secure\\/?$/i",   "target":"\\Mod\\Page::hookSecurePage", "description":"Sets whether a page is secure"},
        {"rank":200, "hook":"/^web:\\/?api\\/page\\/checkpid\\/?$/i", "target":"\\Mod\\Page::hookCheckPid",   "description":"Checks if supplied Page ID is valid"},
        {"rank":300, "hook":"/(?!.*\\.[a-z]*$)^web:.*$/i",            "target":"\\Mod\\Page::hookHeadOpen",   "description":"Open head and page for remaining hooks (without extensions)"},
        {"rank":320, "hook":"/(?!.*\\.[a-z]*$)^web:.*$/i",            "target":"\\Mod\\Page::hookMain",       "description":"Close head and open body"},
        {"rank":390, "hook":"/(?!.*\\.[a-z]*$)^web:.*$/i",            "target":"\\Mod\\Page::hookMenu",       "description":"Page module menu entries"},
        {"rank":498, "hook":"/(?!.*\\.[a-z]*$)^web:.*$/i",            "target":"\\Mod\\Page::hookBodyClose",  "description":"Close body and page"}
    ],
    
    "module_placeholders": {
        "placeholders": [
            {"hook":"/^pageid$/i", "target":"\\Mod\\Page::placeholderPageId"},
            {"hook":"/^pagerevision$/i", "target":"\\Mod\\Page::placeholderPageRevision"},
            {"hook":"/^sitemap$/i", "target":"\\Mod\\Page::placeholderSitemap"}
        ]
    }
}